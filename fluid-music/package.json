{
  "name": "fluid-music",
  "version": "0.3.1",
  "description": "Basic cybr client examples",
  "main": "built/src/index.js",
  "types": "built/src/index.d.ts",
  "scripts": {
    "watch": "nodemon -e .js,.mjs,.json,.yaml,.yml src/index.js",
    "test": "mocha",
    "test-sess": "mocha test/test-session.js",
    "test-tab": "mocha test/test-tab.js",
    "test-gen": "mocha test/test-code-generators.js",
    "test-debug": "mocha --reporter spec --inspect-brk",
    "test-elib": "mocha test/test-event-library.js",
    "test-ipc-client": "mocha test/test-ipc-client.js",
    "docs": "jsdoc2md  --files $(find src -iname '*.js' | grep -v \"plugin-\" | xargs) > docs/fluid.md",
    "generate-zebra2-code": "node code-generators/gen-zebra2-vst2-file.js",
    "generate-helm-code": "node code-generators/gen-helm-file.js"
  },
  "author": "Charles Holbrow",
  "license": "UNLICENSED",
  "devDependencies": {
    "jsdoc-to-markdown": "^5.0.3",
    "mocha": "^6.2.3",
    "nodemon": "^2.0.3",
    "should": "^13.2.3",
    "typescript": "^3.9.7"
  },
  "dependencies": {
    "@types/node": "^14.6.0",
    "expr-eval": "^2.0.2",
    "osc-ipc-client": "^1.1.3",
    "osc-min": "^1.1.2",
    "ramda": "^0.27.0",
    "rppp": "^0.1.3",
    "sharp11": "^2.0.2"
  },
  "bin": {
    "fluid": "./cli.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/charlesholbrow/cybr.git"
  }
}
